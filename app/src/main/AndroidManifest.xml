<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test349">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Test349">
        <activity android:name="com.example.test349.Account"></activity>
        <activity android:name="com.example.test349.Search" />
        <activity android:name="com.example.test349.Message" />
        <activity android:name="com.example.test349.Home" />
        <activity android:name="com.example.test349.Weryfication_register" />
        <activity android:name="com.example.test349.Register_activity" />
        <activity
            android:name="com.example.test349.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>